@model GraysPavers_Models.OrderHeader 


<div class="text-center">
    
    
    @if (Model != null)
    {
        <h1 class="text-info">Order Created</h1>
        <p>Order #@Model.Id has been created successfully! </p>
    }
    else
    {
        <h1 class="text-info">Inquiry Confirmation</h1>
    
    
        <p>We have received your Inquiry request. Someone from our team will contact you shortly!</p>
    

    }

    <img src="~/images/product/Serene.jfif" width="80%"/>
    
    
    

</div>